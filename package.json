    "dist",
    "templates",
    "index.js"
  ],
  "main": "index.mjs",
  "engines": {
    "node": "^14.18.0 || >=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vexip-ui/create-vexip.git"
  },
  "homepage": "https://github.com/vexip-ui/create-vexip",
  "dependencies": {
    "kolorist": "^1.6.0",
    "minimist": "^1.2.7",
    "prompts": "^2.4.2",
    "taze": "^0.8.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@types/fs-extra": "^9.0.13",
    "@types/minimist": "^1.2.2",
    "@types/node": "^18.11.0",
    "@types/prompts": "^2.4.1",
    "@types/semver": "^7.3.12",
    "@vexip-ui/commitlint-config": "^0.1.0",
    "@vexip-ui/eslint-config": "^0.3.1",
    "@vexip-ui/icons": "^1.0.3",
    "@vexip-ui/plugins": "^1.2.0",
    "@vexip-ui/prettier-config": "^0.1.0",
    "@vexip-ui/stylelint-config": "^0.1.1",
    "@vexip-ui/utils": "^1.7.0",
    "@vitejs/plugin-vue": "^3.1.2",
    "@vitejs/plugin-vue-jsx": "^2.0.1",
    "conventional-changelog-cli": "^2.2.2",
    "eslint": "^8.25.0",
    "execa": "^6.1.0",
    "fs-extra": "^10.1.0",
    "husky": "^8.0.1",
    "is-ci": "^3.0.1",
    "lint-staged": "^13.0.3",
    "npm-check-updates": "^16.3.11",
    "pinia": "^2.0.23",
    "prettier": "^2.7.1",
    "semver": "^7.3.8",
    "stylelint": "^14.13.0",
    "stylelint-prettier": "^2.0.0",
    "tsup": "^6.2.3",
    "tsx": "^3.10.1",
    "typescript": "^4.8.4",
    "unbuild": "^0.9.1",
    "unplugin-auto-import": "^0.11.2",
    "unplugin-vue-components": "^0.22.8",
    "vexip-ui": "^2.0.6",
    "vite": "^3.1.8",
    "vitest": "^0.24.3",
    "vue": "^3.2.41",
    "vue-router": "^4.1.5"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "rollup": "^3.0.0"
      }
    }
  }
